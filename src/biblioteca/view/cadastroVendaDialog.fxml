<AnchorPane id="AnchorPane" prefHeight="321.0" prefWidth="473.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="biblioteca.controller.CadastroVendaDialogController">
    <stylesheets>
        <URL value="@/biblioteca/css/cadastrovendadialog.css" />
    </stylesheets>
   <children>
      <Group>
         <children>
            <Button fx:id="buttonCancelar" layoutX="395.0" layoutY="282.0" mnemonicParsing="false" text="Cancelar" />
            <Button fx:id="buttonConfirmar" layoutX="308.0" layoutY="282.0" mnemonicParsing="false" text="Confirmar" />
         </children>
      </Group>
      <GridPane layoutX="14.0" layoutY="14.0" prefHeight="235.0" prefWidth="447.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="218.0" minWidth="10.0" prefWidth="118.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="329.0" minWidth="10.0" prefWidth="329.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="38.0" minHeight="10.0" prefHeight="27.0" vgrow="SOMETIMES" />
